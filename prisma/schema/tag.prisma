model Tag {
    id Int @id @default(autoincrement())
    name String @unique @db.varChar(96)
    slug String @unique @db.varChar(512)
    description String? @db.Text
    schema String? @db.Text
    featureImage String? @db.varChar(1024)
    createDate DateTime @default(now())
    updateDate DateTime
    deleteDate DateTime
    posts Post[] @relation("PostTags", onDelete: Cascade)
}